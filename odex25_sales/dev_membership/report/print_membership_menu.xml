<odoo>

    <record id="paperformat_membership_card" model="report.paperformat">
        <field name="name">Membership Card</field>
        <field name="default" eval="True" />
        <field name="format">custom</field>
        <field name="page_height">130</field>
<!--        <field name="page_height">102</field>-->
        <field name="page_width">172</field>
<!--        <field name="page_width">152</field>-->
        <field name="orientation">Portrait</field>
        <field name="margin_top">25</field>
        <field name="margin_bottom">7</field>
        <field name="margin_left">0</field>
        <field name="margin_right">0</field>
        <field name="header_line" eval="False" />
        <field name="header_spacing">25</field>
        <field name="dpi">90</field>
    </record>

    <record id="menu_print_membership" model="ir.actions.report">
        <field name="name">Print Membership</field>
        <field name="model">dev.membership</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">dev_membership.membership_card</field>
        <field name="report_file">dev_membership.membership_card</field>
        <field name="binding_model_id" ref="dev_membership.model_dev_membership"/>
        <field name="binding_type">report</field>
        <field name="paperformat_id" ref="dev_membership.paperformat_membership_card"/>
    </record>


    <record id="menu_print_membership_nomination" model="ir.actions.report">
        <field name="name">Print Membership Nomination</field>
        <field name="model">board.membership.nomination</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">dev_membership.membership_nomination_template</field>
        <field name="report_file">dev_membership.membership_nomination_template</field>
        <field name="binding_model_id" ref="dev_membership.model_board_membership_nomination"/>
        <field name="binding_type">report</field>
<!--        <field name="paperformat_id" ref="dev_membership.paperformat_membership_card"/>-->
    </record>
</odoo>